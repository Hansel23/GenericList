  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Benjamin Bruska",
      "email": "benjamin.bruska@gmx.de"
    }
  ],

  "require": {
      "php": ">=5.5"
  },

  "require-dev": {
      "codeception/codeception": "2.*",
      "satooshi/php-coveralls": "dev-master"
  },

  "autoload": {
